{
  "name": "swat-proxy",
  "version": "1.0.0",
  "description": "A NodeJS proxy server.",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist",
    "eslint": "eslint index.js src/**",
    "postinstall": "npm run build",
    "precommit": "tape -r babel-register 'test/**/*.spec.js' | tap-spec && npm run eslint",
    "start": "npm run build && node dist/proxy-runner.js",
    "test": "tape -r babel-register 'test/**/*.spec.js' | tap-spec || true"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/bazaarvoice/swat-proxy.git"
  },
  "engines": {
    "node": "5.6.0",
    "npm": "3.6.0"
  },
  "author": "swat@bazaarvoice.com",
  "license": "SEE LICENSE IN LICENSE.md",
  "dependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-node5": "^11.0.2",
    "cheerio": "^0.20.0",
    "request": "^2.72.0"
  },
  "devDependencies": {
    "eslint": "^2.9.0",
    "eslint-plugin-react": "^5.1.1",
    "tap-spec": "^4.1.1",
    "tape": "^4.5.1"
  }
}
